	number = {2},
	journaltitle = {European Journal of Dentistry},
	shortjournal = {Eur J Dent},
	author = {Sari, Desi Sandra and Maduratna, Ernie and Latief, Fourier Dzar Eljabbar and Nugraha, Alexander Patera and Sudiana, Ketut and Rantam, Fedik Abdul and Ferdiansyah and Satuman},
	urldate = {2020-01-24},
	date = {2019-05},
	langid = {english},
