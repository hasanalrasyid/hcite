	number = {1},
	journaltitle = {{AIP} Conference Proceedings},
	shortjournal = {{AIP} Conference Proceedings},
	author = {Syarifah, Ratna Dewi and Suud, Zaki},
	urldate = {2017-10-11},
	date = {2015-09-30},
}

