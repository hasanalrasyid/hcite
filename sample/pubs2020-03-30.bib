	shortjournal = {Mater. Res. Express},
	author = {Husnah, Miftahul and Fakhri, Hafizh A. and Rohman, F. and Aimon, Akfiny H. and Iskandar, Ferry},
	urldate = {2017-10-17},
	date = {2017},
	langid = {english}
}

