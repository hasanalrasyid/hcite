@article{islahuddin_mathematical_2015,
	title = {Mathematical modeling of synthetic unit hydrograph case study: Citarum watershed},
	volume = {1677},
	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4930643},
	doi = {10.1063/1.4930643},
	shorttitle = {Mathematical modeling of synthetic unit hydrograph case study},
	abstract = {Deriving unit hydrograph is very important in analyzing watershed’s hydrologic response of a rainfall event. In most cases, hourly measures of stream flow data needed in deriving unit hydrograph are not always available. Hence, one needs to develop methods for deriving unit hydrograph for ungagged watershed. Methods that have evolved are based on theoretical or empirical formulas relating hydrograph peak discharge and timing to watershed characteristics. These are usually referred to Synthetic Unit Hydrograph. In this paper, a gamma probability density function and its variant are used as mathematical approximations of a unit hydrograph for Citarum Watershed. The model is adjusted with real field condition by translation and scaling. Optimal parameters are determined by using Particle Swarm Optimization method with weighted objective function. With these models, a synthetic unit hydrograph can be developed and hydrologic parameters can be well predicted.},
	pages = {030021},
