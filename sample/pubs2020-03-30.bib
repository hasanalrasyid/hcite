	author = {Indrasari, Widyaningrum and Djamal, Mitra and Srigutomo, Wahyu and {Ramli}},
	urldate = {2017-10-13},
	date = {2016-03-11},
}

@article{waris_preliminary_2016-1,
	title = {Preliminary design of betavoltaic battery using Co-60 and Pm-147 with {GaAs} substrate},
	volume = {1719},
	issn = {0094-243X},
