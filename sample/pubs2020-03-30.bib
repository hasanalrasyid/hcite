	issue = {Supplement C},
	journaltitle = {Journal of Colloid and Interface Science},
	shortjournal = {Journal of Colloid and Interface Science},
	author = {Kamarudin, N. H. N. and Jalil, A. A. and Triwahyono, S. and Artika, V. and Salleh, N. F. M. and Karim, A. H. and Jaafar, N. F. and Sazegar, M. R. and Mukti, R. R. and Hameed, B. H. and Johari, A.},
	urldate = {2017-10-10},
	date = {2014-05-01},
	keywords = {Mesoporous silica nanoparticles, Ibuprofen, Drug delivery, Microwave, {pH}},
}

@article{ng_detailed_2015,
	title = {Detailed kinetic observation revealing the formation mechanism of chiral mesoporous silica ({CMS}) synthesized by cooperative self-assembly of anionic chiral surfactant},
	volume = {62},
