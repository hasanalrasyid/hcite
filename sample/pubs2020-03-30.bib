	number = {10},
	journaltitle = {Advanced Science, Engineering and Medicine},
	shortjournal = {Advanced Science, Engineering and Medicine},
	author = {Anam, Choirul and Haryanto, Freddy and Widita, Rena and Arif, Idam},
