	number = {1},
	journaltitle = {Metallurgical and Materials Transactions B},
	shortjournal = {Metall and Materi Trans B},
	author = {Hidayat, Taufiq and Chen, Jiang and Hayes, Peter C. and Jak, Evgueni},
	urldate = {2020-01-24},
	date = {2019-02-01},
	langid = {english}
}

