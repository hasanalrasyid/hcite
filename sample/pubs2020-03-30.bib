}

@article{andiwijayakusuma_density_2016,
	title = {Density functional theory study : Electronic structures of {RE}:{GaN} in wurtzite G α15 {RE} 1 N 16},
	volume = {739},
	issn = {1742-6596},
	url = {http://stacks.iop.org/1742-6596/739/i=1/a=012027},
