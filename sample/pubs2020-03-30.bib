@article{pancahayani_directed_2014,
	title = {Directed Metric Dimension of Oriented Graphs with Cyclic Covering},
	url = {http://arxiv.org/abs/1401.0929},
	abstract = {Let \$D\$ be a strongly connected oriented graph with vertex-set \$V\$ and arc-set \$A\$. The distance from a vertex \$u\$ to another vertex \$v\$, \$d(u,v)\$ is the minimum length of oriented paths from \$u\$ to \$v\$. Suppose \$B={\textbackslash}\{b\_1,b\_2,b\_3,...b\_k{\textbackslash}\}\$ is a nonempty ordered subset of \$V\$. The representation of a vertex \$v\$ with respect to \$B\$, \$r(v{\textbar}B)\$, is defined as a vector \$(d(v,b\_1), d(v,b\_2), ..., d(v,b\_k))\$. If any two distinct vertices \$u,v\$ satisfy \$r(u{\textbar}B){\textbackslash}neq r(v{\textbar}B)\$, then \$B\$ is said to be a resolving set of \$D\$. If the cardinality of \$B\$ is minimum then \$B\$ is said to be a basis of \$D\$ and the cardinality of \$B\$ is called the directed metric dimension of \$D\$. Let \$G\$ be the underlying graph of \$D\$ admitting a \$C\_n\$-covering. A \$C\_n\$-simple orientation is an orientation on \$G\$ such that every \$C\_n\$ in \$D\$ is strongly connected. This paper deals with metric dimensions of oriented wheels, oriented fans, and amalgamation of oriented cycles, all of which admitting \$C\_n\$-simple orientations.},
	journaltitle = {{arXiv}:1401.0929 [math]},
	author = {Pancahayani, Sigit and Simanjuntak, Rinovia},
	urldate = {2017-10-11},
	date = {2014-01-05},
	eprinttype = {arxiv},
	eprint = {1401.0929},
	keywords = {Mathematics - Combinatorics, 05C12},
}

