	number = {10},
	journaltitle = {Macromolecular Chemistry and Physics},
	shortjournal = {Macromol. Chem. Phys.},
	author = {Rachmawati, Rachmawati and de Gier, Hilde D. and Woortman, Albert J. J. and Loos, Katja},
	urldate = {2017-10-11},
