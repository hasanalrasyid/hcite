	shortjournal = {{AIP} Conference Proceedings},
	author = {Dermawan, B. and Huda, I. N. and Hidayat, T. and Mandey, D. and Utama, J. A. and Wibowo, R. W. and Tampubolon, I.},
	urldate = {2017-10-11},
	date = {2015-09-30},
}

@article{hidayat_evaluation_2015,
	title = {Evaluation of aerosol contents over astronomical candidate site in Indonesia from meteorological satellite data},
