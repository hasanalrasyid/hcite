@book{hussain_d-vertex-antimagic_2010,
	title = {On (a,d)-vertex-antimagic total labeling of Harary graphs},
	volume = {83},
	abstract = {Let G=(V,E) be a graph with v vertices and e edges. An (a,d)-vertex-antimagic total labeling is a bijection λ from V(G)∪E(G) to the set of consecutive integers 1,2,⋯,v+e, such that the weights of the vertices form an arithmetic progression with the initial term a and common difference d. If λ(V(G))=\{1,2,⋯,v\} then we call the labeling a super (a,d)-vertex- antimagic total. We construct (a,d)-vertex-antimagic total labelings on Harary graphs as well as or the disjoint union of k identical copies of Harary graphs.},
