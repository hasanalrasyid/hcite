@article{dermawan_estimation_2015,
	title = {Estimation of unknown physical properties of near-Earth asteroids for dynamical study},
	volume = {1677},
	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4930668},
