@article{anam_investigation_2019,
	title = {An investigation of a {CT} noise reduction using a modified of wiener filtering-edge detection},
	volume = {1217},
	issn = {1742-6596},
	url = {https://doi.org/10.1088%2F1742-6596%2F1217%2F1%2F012022},
	doi = {10.1088/1742-6596/1217/1/012022},
	abstract = {The aims of this study were to investigate the noise reduction in a {CT} image using a modified Wiener filtering-edge detection method. We modified the noise reduction algorithm of a combination of the Wiener filter and edge detection by addition of a dilation stage after edge detection. We then evaluated kernel size of the Wiener filter, threshold values in the edge detection, and size of structuring elements in the dilation process. Images of adult anthropomorphic and self-built wire phantoms were acquired by the new 4-row multislice {CT} Toshiba Alexion™. The images of the anthropomorphic phantom were used for a visual evaluation, while the images of the wire-phantom were used to obtain the spatial resolution and noise of the images. A Wiener filter-edge detection filter coupled with dilation, potentially reduced more {CT} noise. We found that the spatial resolution and noise of the filtered images were influenced by the size of the Wiener filter kernel, threshold of edge detection, and size of structuring element.},
	pages = {012022},
	journaltitle = {Journal of Physics: Conference Series},
	shortjournal = {J. Phys.: Conf. Ser.},
	author = {Anam, C. and Fujibuchi, T. and Toyoda, T. and Sato, N. and Haryanto, F. and Widita, R. and Arif, I. and Dougherty, G.},
	urldate = {2020-01-24},
	date = {2019-05},
	langid = {english},
}

@article{anam_investigation_2019-1,
	title = {An investigation of spatial resolution and noise in reconstructed {CT} images using iterative reconstruction ({IR}) and filtered back-projection ({FBP})},
	volume = {1127},
	issn = {1742-6596},
	url = {https://doi.org/10.1088%2F1742-6596%2F1127%2F1%2F012016},
	doi = {10.1088/1742-6596/1127/1/012016},
	abstract = {This study compared the spatial resolution and the amount of noise in {CT} images reconstructed using the iterative reconstruction ({IR}) and filtered back-projection ({FBP}) techniques. We used a circular homogeneous phantom (pixel value of 0.5) containing a small circle at its center, with a diameter of 0.53-cm and a pixel value of 1.0. In order to reconstruct the image, we used the simultaneous iterative reconstruction technique ({SIRT}) with 5, 15, 25, 35 and 45 iterations respectively, and {FBP} with a Ram-Lak filter. The spatial resolution of the reconstructed image was characterized by the modulation transfer function ({MTF}) and image noise was characterized by the standard deviation within the homogeneous area. We found that the spatial resolution using {SIRT} with 35 iterations is similar to using {FBP}. The spatial resolution using {SIRT} tends to be worse using less than 35 iterations, and it tends to be better using more than 35 iterations. Using {SIRT}, image noise decreased with increasing iterations. The standard deviation for the {SIRT} technique using 45 iterations was about 0.02 similar to the {FBP} technique. Thus, {SIRT} could potentially produce better spatial resolution than the {FBP} technique with comparable image noise.},
	pages = {012016},
	journaltitle = {Journal of Physics: Conference Series},
	shortjournal = {J. Phys.: Conf. Ser.},
	author = {Anam, C. and Haryanto, F. and Widita, R. and Arif, I. and Dougherty, G.},
	urldate = {2020-01-24},
	date = {2019-01},
	langid = {english},
}

