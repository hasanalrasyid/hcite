@article{aruan_polyvinyl_2017,
	title = {Polyvinyl Alcohol/Soursop Leaves Extract Composite Nanofibers Synthesized Using Electrospinning Technique and their Potential as Antibacterial Wound Dressing},
	volume = {170},
	issn = {1877-7058},
	url = {http://www.sciencedirect.com/science/article/pii/S1877705817311116},
