	shortjournal = {{AIP} Conference Proceedings},
	author = {Aji, A. S. and Darma, Y.},
	urldate = {2017-10-06},
	date = {2014-03-24},
}

@article{hartono_effect_2014,
	title = {Effect of roll hot press temperature on crystallite size of {PVDF} film},
