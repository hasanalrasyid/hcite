}

@article{suud_safety_2017,
	title = {Safety Analysis of Pb-208 Cooled 800 {MWt} Modified {CANDLE} Reactors},
	volume = {799},
	issn = {1742-6596},
	url = {http://stacks.iop.org/1742-6596/799/i=1/a=012013},
	doi = {10.1088/1742-6596/799/1/012013},
	abstract = {Safely analysis of 800MWt Pb-208 cooled fast reactors with natural Uranium as fuel cycle input employing axial-radial combined Modiified {CANDLE} burnup scheme has been performed. The analysis of unprotected loss of flow({ULOF}) and unprotected rod run-out transient overpower ({UTOP}) are discussed. Some simulations for 800 {MWt} Pb-208 cooled fast reactors has been performed and the results show that the reactor can anticipate complete pumping failure inherently by reducing power through reactivity feedback and remove the rest of heat through natural circulations. Compared to the Pb-nat cooled long life Fast Reactors, Pb-208 cooled reactors have smaller Doppler but higher coolant density reactivity coefficient. In the {UTOP} accident case the analysis has been performed against external reactivity up to 0.003dk/k. And for {ULOHS} case it is assumed that the secondary cooling system has broken. During all accident the cladding temperature is the most critical. Especially for the case of {UTOP} accident. In addition the steam generator design has also consider excess power which may reach 50\% extra during severe {UTOP} case..},
