	number = {1},
	journaltitle = {{AIP} Conference Proceedings},
	shortjournal = {{AIP} Conference Proceedings},
	author = {Evita, Maria and Djamal, Mitra and Zimanowski, Bernd and Schilling, Klaus},
	urldate = {2017-10-17},
	date = {2017-01-10},
}

