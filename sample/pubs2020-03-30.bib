	journaltitle = {Applied Surface Science},
	shortjournal = {Applied Surface Science},
	author = {Jusoh, N. W. C. and Jalil, A. A. and Triwahyono, S. and Karim, A. H. and Salleh, N. F. and Annuar, N. H. R. and Jaafar, N. F. and Firmansyah, M. L. and Mukti, R. R. and Ali, M. W.},
