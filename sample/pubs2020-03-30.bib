@article{atarita_anisotropy_2019,
	title = {Anisotropy of Magnetic Susceptibility and Preferred Pore Orientation in Lava Flow from the Ijen Volcanic Complex, East Java, Indonesia},
	volume = {9},
	rights = {http://creativecommons.org/licenses/by/3.0/},
	url = {https://www.mdpi.com/2076-3263/9/7/304},
	doi = {10.3390/geosciences9070304},
	abstract = {Anisotropy of magnetic susceptibility ({AMS}) has been used in various studies related to interpreting the direction of lava flow, some of which have shown ambiguity with regard to the data generated. In this study, we explored an alternative option to support the aforementioned application, using lava flow type igneous rock samples from the Ijen Volcanic Complex, East Java, Indonesia. We have investigated the preferred rock pore orientations from micro-computed tomography (\&mu;{CT}) images and quantified their directions. We then calculated their correlation with {AMS} data by calculating the angle between preferred pore orientation. The axis with the smallest gap to the preferred pore orientation of each sample was assumed to imply lava flow direction. Different lava flow direction preferences were obtained from different magnetic ellipsoids. Another important factor for consideration is the relative vertical position of the sampling site within a single lava flow unit. Only one out of five samples ({ANY}2) show good quantitative conformity between {AMS} data, preferred pore orientation, and topographical slope, despite these limitations. Our results point to a direction that seems to be correct and coherent on a physical basis. Additional research would likely clarify the issues involved. This encourages us to explore and work further in this field of research.},
	pages = {304},
	number = {7},
	journaltitle = {Geosciences},
	author = {Atarita, Fadhli Ramadhana and Bijaksana, Satria and Ndari, Nuresi Rantri Desi Wulan and Pratama, Aditya and Taqwantara, Reyhan Fariz and Fajar, Silvia Jannatul and Latief, Fourier Dzar Eljabbar},
	urldate = {2020-01-24},
	date = {2019-07},
	langid = {english},
	keywords = {anisotropy of magnetic susceptibility ({AMS}), igneous rock, lava flow direction, preferred pore orientation},
}

