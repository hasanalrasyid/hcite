	journaltitle = {Journal of Physics: Conference Series},
	shortjournal = {J. Phys.: Conf. Ser.},
	author = {Arkundato, A. and Monado, F. and {Supeno} and {Misto} and Su'ud, Z.},
	urldate = {2020-01-24},
	date = {2019-03},
	langid = {english},
}

