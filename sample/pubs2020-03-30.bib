	keywords = {micro-Raman, {YbVO}3, phonon, orbiton, phase transitions},
}

@article{nakatsuji_spin-orbital_2012,
	title = {Spin-Orbital Short-Range Order on a Honeycomb-Based Lattice},
	volume = {336},
