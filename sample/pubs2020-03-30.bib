	journaltitle = {Journal of Mathematical and Fundamental Sciences},
	author = {Noviyanti, Atiek Rostika and Prijamboedi, Bambang and Marsih, I. Nyoman and Ismu, Ismunandar},
	urldate = {2017-09-28},
