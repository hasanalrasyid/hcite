@article{sebastian_spectral_2019,
	title = {Spectral Analysis of Volume Operators in Loop Quantum Gravity for Kinematical Case},
	volume = {1245},
	issn = {1742-6596},
	url = {https://doi.org/10.1088%2F1742-6596%2F1245%2F1%2F012086},
	doi = {10.1088/1742-6596/1245/1/012086},
	abstract = {Loop Quantum Gravity has become one of the alternative solutions to quantum gravity. This formulation introduced geometrical operators which successfully used to model that in the quantum scale, the space is actually discretized in the order of Planck length. These operators are area and volume operator. The regularization process of these operators came from the classical definition of area and volume, thus, the eigenvalues of area operator and volume operator are respectively the area and volume of the space. However, there exists two types of volume operator, the Ashtekar-Lewandowski operator and the Rovelli-Smolin operator. The significant difference between these two operators is the fact that Ashtekar-Lewandowski operator is sensitive to the direction of the spin networks link, while Rovelli-Smolin operator is not. This difference will produce different spectral. In this article, we compare the resulting spectral of the two volume operators, where both of them is used to calculate the volume of the monochromatic 4-valent and 6-valent spin network for the kinematical case.},
	pages = {012086},
	journaltitle = {Journal of Physics: Conference Series},
	shortjournal = {J. Phys.: Conf. Ser.},
	author = {Sebastian, Ignatius and Husin, Idrus and Ariwahjoedi, Seramika and Zen, Freddy P.},
	urldate = {2020-01-24},
	date = {2019-08},
	langid = {english},
}

