	keywords = {Internet of Things, agricultural land, agricultural production capacity, agriculture, Agriculture, bok choy plants, control engineering computing, controlling system, crops, fuzzy control, fuzzy logic, Fuzzy logic, hydroculture, hydroponics, hydroponics precision agriculture, intelligent control, intelligent monitoring, {IoT}, lettuce, Microcontrollers, Monitoring, plant nutrition, Temperature measurement, Temperature sensors, Valves},
}

@article{jenni_interplay_2019,
	title = {Interplay of Electronic and Spin Degrees in Ferromagnetic \$\{{\textbackslash}mathrm\{{SrRuO}\}\}\_\{3\}\$: Anomalous Softening of the Magnon Gap and Stiffness},
	volume = {123},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.123.017202},
	doi = {10.1103/PhysRevLett.123.017202},
	shorttitle = {Interplay of Electronic and Spin Degrees in Ferromagnetic \$\{{\textbackslash}mathrm\{{SrRuO}\}\}\_\{3\}\$},
	abstract = {The magnon dispersion of ferromagnetic {SrRuO}3 was studied by inelastic neutron scattering experiments on single crystals as a function of temperature. Even at low temperature the magnon modes exhibit substantial broadening pointing to strong interaction with charge carriers. We find an anomalous temperature dependence of both the magnon gap and the magnon stiffness, which soften upon cooling in the ferromagnetic phase. Both effects trace the temperature dependence of the anomalous Hall effect and can be attributed to the impact of Weyl points, which results in the same relative renormalization in the spin stiffness and magnon gap.},
