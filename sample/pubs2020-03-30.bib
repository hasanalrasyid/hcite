	author = {Yang, Chengbin and Chan, Kok Ken and Xu, Gaixia and Yin, Mingjie and Lin, Guimiao and Wang, Xiaomei and Lin, Wei-Jen and Birowosuto, Muhammad Danang and Zeng, Shuwen and Ogi, Takashi and Okuyama, Kikuo and Permatasari, Fitri Aulia and Iskandar, Ferry and Chen, Chih-Kuang and Yong, Ken-Tye},
	urldate = {2020-01-27},
	date = {2019-01-23},
}

@article{amalia_coating_2019,
	title = {Coating of {TiO}2 nanoparticles in the surface of transparent plastic grains and its aplication for photocatalyst under solar irraidation},
