	number = {1},
	urldate = {2017-10-17},
	journal = {J. Phys.: Conf. Ser.},
	author = {Wirawan, Rahadi and Waris, Abdul and Djamal, Mitra and Gunawan, H. and Kim, H. J.},
	year = {2017},
	pages = {012010},
}

@article{nasrudin_charged_2017,
