@article{syarif_synthesis_2010,
	title = {Synthesis of {8YSZ}‐{LSGM} {Composite} {Thick} {Film} {Ceramics} for {Solid} {Electrolyte} {From} {Nanopowder} {Utilizing} {Local} {Zircon} {Prepared} {Using} {Sol} {Gel} {Process}},
