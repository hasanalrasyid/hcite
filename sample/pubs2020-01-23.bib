	title = {On {Locating}-chromatic {Number} for {Graphs} with {Dominant} {Vertices}},
	volume = {74},
	issn = {1877-0509},
	url = {http://www.sciencedirect.com/science/article/pii/S1877050915035425},
	doi = {10.1016/j.procs.2015.12.081},
	abstract = {Let c be a k-coloring of a (not necessary connected) graph H. Let П= \{C1, C2, · · ·, Ck \} be the partition of V(H) induced by c, where Ci is partition class receiving color i. The color code cП(v) of a vertex v ∈ H is the ordered k-tuple (d(v, C1), d(v, C2), · · ·, d(v, Ck )), where d(v, Ci) = min\{d(v, x){\textbar}x ∈ Ci\} for all i ∈ [1,k]. If all vertices of H have distinct color codes, then c is called a locating k-coloring of H. The locating-chromatic number of H, denoted by χL’ (H), is the smallest k such that H admits a locating- coloring with k colors. If there is no integer k satisfying the above conditions, then we say that χ’L (H) = ∞. Note that if H is a connected graph, then χL’ (H) = χL(H). In this paper, we provide upper bounds for the locating-chromatic numbers of connectedgraphs obtained from disconnected graphs where each component contains a single dominant vertex.},
	number = {Supplement C},
	urldate = {2017-10-10},
	journal = {Procedia Computer Science},
	author = {Welyyanti, Des and Baskoro, Edy Tri and Simanjuntak, Rinovia and Uttunggadewa, Saladin},
	month = jan,
	year = {2015},
	keywords = {locating-chromatic number, dominant vertex, coloring, color.},
	pages = {89--92},
}

@article{krismanto_fractional_2015,
	series = {“{The} 2nd {International} {Conference} of {Graph} {Theory} and {Information} {Security}”},
	title = {Fractional {Metric} {Dimension} of {Tree} and {Unicyclic} {Graph}},
	volume = {74},
	issn = {1877-0509},
	url = {http://www.sciencedirect.com/science/article/pii/S1877050915035358},
	doi = {10.1016/j.procs.2015.12.074},
	abstract = {A vertex v in a simple connected graph G resolves two vertices x and y in G if the distance from x to v is not equal to distance from y to v. The vertex set R\{x, y\} is defined as the set of vertices in G which resolve x and y. A function f : V(G) → [0,1] is called a resolving function of G if f (R\{x, y\}) ≥ 1 for any two distinct vertices x and y in G. The minimal value of f (V(G)) for all resolving functions f of G is called the fractional metric dimension of G. In this paper, we determine the fractional metric dimension of Gwhere G is a tree or G is a unicyclic graph.},
	number = {Supplement C},
	urldate = {2017-10-10},
	journal = {Procedia Computer Science},
	author = {Krismanto, Daniel A. and Saputro, Suhadi Wido},
	month = jan,
	year = {2015},
	keywords = {tree graph, Fractional metric dimension, resolving function, unicyclic graph},
	pages = {47--52},
}

