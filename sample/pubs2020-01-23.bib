	doi = {10.1155/S0161171201010675},
}

@article{iswadi_metric_2010,
	title = {The {Metric} {Dimension} of {Amalgamation} of {Cycles}},
	volume = {41},
	issn = {0972-0871},
	url = {http://pphmj.com/journals/fjms.htm},
	abstract = {For an ordered set W = \{w\_1, w\_2 , ..., w\_k \} of vertices and a vertex v in a connected graph G, the representation of v with respect to W is the ordered k-tuple r(v{\textbar}W) = (d(v,w\_1), d(v,w\_2 ), ..., d (v,w\_k )), where d(x,y) represents the distance between the vertices x and y. The set W is called a resolving set for G if every vertex of G has a distinct representation. A resolving set containing a minimum number of vertices is called a basis for G. The dimension of G, denoted by dim(G), is the number of vertices in a basis of G. Let \{G\_i\} be a finite collection of graphs and each G\_i has a fixed vertex voi called a terminal. The amalgamation Amal \{Gi , v\_\{oi\}\} is formed by taking all of the G\_i’s and identifying their terminals. In this paper, we determine the metric dimension of amalgamation of cycles.},
	number = {1},
	urldate = {2017-09-27},
	journal = {Far East Journal of Mathematical Sciences (FJMS)},
	author = {Iswadi, Hazrul and Baskoro, Edy Tri and Salman, A. N. M. and Simanjuntak, Rinovia},
	year = {2010},
	pages = {19--31},
}

@article{siahaan_semiclassical_2010,
