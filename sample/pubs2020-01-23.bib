	author = {Nakatsuji, S. and Kuga, K. and Kimura, K. and Satake, R. and Katayama, N. and Nishibori, E. and Sawa, H. and Ishii, R. and Hagiwara, M. and Bridges, F. and Ito, T. U. and Higemoto, W. and Karaki, Y. and Halim, M. and Nugroho, A. A. and Rodriguez-Rivera, J. A. and Green, M. A. and Broholm, C.},
	month = may,
	year = {2012},
