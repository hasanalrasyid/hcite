	keywords = {energy, Fuel cell, Proton transfer, Proton dissociation, Ab initio calculation, Sulfonated nata de coco},
	pages = {442--447},
}

@article{arutanti_tailored_2016,
	title = {Tailored synthesis of macroporous {Pt}/{WO3} photocatalyst with nanoaggregates via flame assisted spray pyrolysis},
	volume = {62},
	issn = {1547-5905},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/aic.15349/abstract},
	doi = {10.1002/aic.15349},
	abstract = {High-surface-area macroporous WO3 particles with deposited Pt (Pt/WO3) were successfully synthesized for the first time, using flame-assisted spray pyrolysis. Nanoparticle aggregates-like structures (nanoaggregates) were formed, although a salt precursor was used for the synthesis. The macroporous structure was tailored by changing the mass ratio of the polystyrene template to ammonium tungstate pentahydrate. The cavities between the nanoaggregates formed mesopores, which increased the surface area. The presence of meso- and macro-pores in the synthesized Pt/WO3 particles improved their photocatalytic activities in visible-light-induced photodegradation of rhodamine B. The combination of a high surface area and the presence of an in situ-deposited Pt cocatalyst gave a high photodecomposition rate, approximately 9.6 times higher than that achieved with dense WO3 particles. This research provides a promising strategy for synthesizing submicron particles with high surface areas at a high production rate, and is suitable for industrial applications. © 2016 American Institute of Chemical Engineers AIChE J, 62: 3864–3873, 2016},
	language = {en},
	number = {11},
	urldate = {2017-10-13},
	journal = {AIChE J.},
	author = {Arutanti, Osi and Arif, Aditya Farhan and Balgis, Ratna and Ogi, Takashi and Okuyama, Kikuo and Iskandar, Ferry},
	month = nov,
	year = {2016},
	keywords = {Tungsten trioxide, visible light photocatalyst, nanostructured particle, macroporous structure, nanoaggregate},
	pages = {3864--3873},
}

@article{darma_strong_2016,
	title = {Strong {Modification} of {Excitons} and {Optical} {Conductivity} for {Different} {Dielectric} {Environments} in {ZnO} {Films}},
	volume = {8},
	issn = {1943-0655},
	doi = {10.1109/JPHOT.2016.2570419},
	abstract = {We study the evolution of excitons and optical conductivity of ZnO films on two different substrates (Pt and quartz) using three different Cu doping concentrations known as Cu:ZnO. The concentrations used in this paper were 0\% (pure ZnO), 2\%, and 8\% of Cu. The results show that the excitons are strongly observed in the ZnO film on quartz (ZOQ). However, they are significantly suppressed in the ZnO film on Pt (ZOP). These imply that the interactions of the valence itinerant electrons in Pt with ZnO film are responsible for the electronic blocking of electron (e)-hole (h) pairs or excitons. Furthermore, as a function of Cu doping, the screening effects on excitonic states and the new localized interband transition were observed for the ZOP and ZOQ systems. Our results confirm the different origins of the reduction of excitonic effects and the importance of the dielectric environments to strongly modify excitons and optical conductivity in a wide band-gap semiconductor.},
	number = {3},
	journal = {IEEE Photonics Journal},
	author = {Darma, Y. and Marlina, R. and Herng, T. S. and Ding, J. and Rusydi, A.},
	month = jun,
	year = {2016},
	keywords = {Zinc oxide, II-VI semiconductors, copper, dielectric materials, doping profiles, electron-hole recombination, excitons, optical conductivity, platinum, quartz, semiconductor thin films, wide band gap semiconductors, zinc compounds, Cu doping concentrations, Pt, Pt substrates, SiO2, ZnO films, ZnO:Cu, dielectric environments, electron-hole pairs, electronic blocking, excitonic states, localized interband transition, quartz substrates, screening effects, strong exciton modification, valence itinerant electrons, wide band-gap semiconductor, Dielectrics, II-VI semiconductor materials, Optical films, Substrates, Optical properties of photonic materials, Spectroscopy},
	pages = {1--9},
}

@misc{darmayanti_kappa-carrageenan_nodate,
	title = {Kappa-{Carrageenan} as an {Attractive} {Green} {Substitute} for {Polyacrylamide} in {Enhanced} {Oil} {Recovery} {Applications}},
	url = {http://www.ijtech.eng.ui.ac.id/old/index.php/journal/article/view/2869},
	abstract = {The rapidly growing demand for petroleum resources has become a crucial
global problem. Therefore, a more realistic solution is required for oil
production. Enhanced oil recovery (EOR) has become an essential technique to
extract original oil content and maintain oil fields. During this process,
certain viscous polymers are commonly used as mobility control agents. In this
work, we introduce a new class of polymer to address the limitations of
commercial EOR polymers. We successfully extracted kappa-type carrageenan from Eucheuma
cottonii seaweed using demineralized water and ethanol precipitation. The
amount of yield, intrinsic viscosity, and viscosity-average molecular mass of
the extracted carrageenan were 18.64\%, 12.77 dLg-1, and 4.716×105
gmol-1, respectively. Characterizations were done by dynamic
viscosity and rheological measurement, along with a thermal degradation test.
The measurements indicated that kappa-carrageenan
is an attractive green substitute for polyacrylamide, as it showed relatively
high resistance to temperature, shear rate, and salinity compared to
polyacrylamide-based commercial EOR polymers. However, a higher concentration
of carrageenan is still needed to reach the same viscosity as the commercial
polymers.},
	urldate = {2017-10-13},
	author = {Darmayanti, Made Ganesh and Radiman, Cynthia Linaya and Sudarma, I Made},
}

