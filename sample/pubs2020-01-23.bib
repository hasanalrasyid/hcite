@article{rediansyah_static_2015,
	title = {Static electric field mapping using a mosquito racket and baby oil},
	volume = {50},
	issn = {0031-9120},
	url = {http://stacks.iop.org/0031-9120/50/i=6/a=690},
	doi = {10.1088/0031-9120/50/6/690},
	abstract = {The aim of this research was to design a simple experimental device to see electric field force lines using common components which are readily available in everyday life. A solution of baby oil was placed in a plastic container, 4.5 × 4.5 × 1 inches, with both ends of the electrodes (metal wire) immersed in the solution at a depth of 0.2 inches. A mosquito racket was used as a source of voltage, and was connected to the electrodes. Electric field lines were seen through grass seeds, which were sown in the solution of baby oil. The results showed the design tool successfully evokes the lines of force of an electric field from a charge point, two point charges, and two dielectric plates with two polar opposite charges.},
	language = {en},
	number = {6},
	urldate = {2017-10-10},
	journal = {Phys. Educ.},
	author = {Rediansyah, Herfien and {Khairurrijal} and Viridi, Sparisoma},
