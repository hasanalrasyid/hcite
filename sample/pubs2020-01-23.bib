	urldate = {2017-10-17},
	journal = {IOP Conf. Ser.: Mater. Sci. Eng.},
	author = {Aminudin, A. and Tjahyono, D. H. and {Suprijadi} and Djamal, M. and Zaen, R. and Nandiyanto, A. B. D.},
	year = {2017},
	pages = {012137},
}

