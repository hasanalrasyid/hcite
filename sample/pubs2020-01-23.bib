@article{kadja_mesoporogen-free_2016,
	title = {Mesoporogen-free synthesis of hierarchically porous {ZSM}-5 below 100 °{C}},
	volume = {226},
	issn = {1387-1811},
	url = {http://www.sciencedirect.com/science/article/pii/S1387181116000949},
