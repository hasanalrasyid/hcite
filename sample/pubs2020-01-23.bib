	number = {10},
	urldate = {2018-04-23},
	journal = {Advanced Functional Materials},
	author = {Abdullah, M. and Morimoto, T. and Okuyama, K.},
	month = oct,
	year = {2003},
	keywords = {Zinc oxide, Europium doping, Nanoparticles, oxide, Photoluminescence},
	pages = {800--804},
}

