@article{nuryadin_photoluminescence_2014,
	title = {Photoluminescence optimization of {BCNO} phosphors synthesized using citric acid as a carbon source},
	volume = {25},
	doi = {10.1016/j.apt.2014.01.003},
	abstract = {Citric acid was used as carbon source for the optimization of the photoluminescence (PL) performance of boron carbon oxynitride (BCNO) phosphor. Citric acid was chosen as an alternative carbon source because of its simple molecular structure, low decomposition temperature, relative inexpensiveness, and environmental friendliness. The prepared sample exhibited a single, homogeneous, and broad photoluminescence emission band whose peak varied from near-UV (400 nm) to yellow-visible (500 nm) upon excitation at 365 nm. The effects of varying the synthesis temperature, molar ratio of the carbon/boron and nitrogen/boron sources, and addition of SiO2 nanoparticles on the PL properties were also studied. The optimized BCNO phosphors may find potential use in white LED applications.},
	journal = {Advanced Powder Technology},
