@article{tuwankotta_two_2015,
	title = {{TWO} {ASPECTS} {OF} {A} {GENERALIZED} {FIBONACCI} {SEQUENCE}},
	volume = {21},
	issn = {2460-0245},
	url = {http://www.jims-a.org/index.php/jimsa/article/view/173},
	doi = {10.22342/jims.21.1.173.1-17},
	abstract = {In this paper we study the so-called generalized Fibonacci sequence: \$x\_\{n+2\} = {\textbackslash}alpha x\_\{n+1\} + {\textbackslash}beta x\_n, n{\textbackslash}in {\textbackslash}mathbb\{N\}\$.  We derive an open domain around the origin of the parameter space where the sequence converges to \$0\$.  The limiting behavior on the boundary of this domain are: convergence to a nontrivial limit, \$k\$-periodic (\$k{\textbackslash}in {\textbackslash}mathbb\{N\}\$), or quasi-periodic.  We use the ratio of two consecutive terms of the sequence to construct a rational approximation for algebraic numbers of the form: \${\textbackslash}sqrt\{r\}, r{\textbackslash}in {\textbackslash}mathbb\{Q\}\$.  Using a similar idea, we extend this to higher dimension to construct a rational approximation for  \${\textbackslash}sqrt[3]\{ a + b{\textbackslash}sqrt\{c\}\} +  {\textbackslash}sqrt[3]\{ a - b{\textbackslash}sqrt\{c\}\} + d\$.  DOI :  http://dx.doi.org/10.22342/jims.21.1.173.1-17},
	language = {en},
	number = {1},
	urldate = {2017-10-10},
	journal = {Journal of the Indonesian Mathematical Society},
	author = {Tuwankotta, Johan Matheus},
	month = jan,
	year = {2015},
	pages = {1--17},
}

@article{astuti_linear_2014,
	title = {Linear transformations with characteristic subspaces that are not hyperinvariant},
	url = {http://arxiv.org/abs/1401.3367},
	abstract = {If \$f\$ is an endomorphism of a finite dimensional vector space over a field \$K\$ then an invariant subspace \$X {\textbackslash}subseteq V\$ is called hyperinvariant (respectively, characteristic) if \$X\$ is invariant under all endomorphisms (respectively, automorphisms) that commute with \$f\$. According to Shoda (Math. Zeit. 31, 611--624, 1930) only if \${\textbar}K{\textbar} = 2\$ then there exist endomorphisms \$f\$ with invariant subspaces that are characteristic but not hyperinvariant. In this paper we obtain a description of the set of all characteristic non-hyperinvariant subspaces for nilpotent maps \$f\$ with exactly two unrepeated elementary divisors.},
	urldate = {2017-10-10},
	journal = {arXiv:1401.3367 [math]},
	author = {Astuti, Pudji and Wimmer, Harald K.},
	month = jan,
	year = {2014},
