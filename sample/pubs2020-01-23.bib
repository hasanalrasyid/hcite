	journal = {J. Phys.: Conf. Ser.},
	author = {Sibit, Denniswara and Handayani, Gunawan},
	year = {2016},
	pages = {012051}
}

@article{wibowo_experimental_2016,
	title = {Experimental study of granular convection in (real) two- dimension {Brazil}-nut effect},
