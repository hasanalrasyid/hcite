	urldate = {2017-10-02},
	journal = {Procedia - Social and Behavioral Sciences},
	author = {Sumarti, Novriana and Fitriyani, Vina and Damayanti, Merisa},
	month = feb,
	year = {2014},
	keywords = {Mathematical Models, data simulation, optimization techniques, micro-credit scheme},
	pages = {131--137},
}

@article{aldila_optimal_2013,
