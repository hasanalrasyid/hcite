@article{handayani_treatment_2014,
	title = {Treatment model of dengue hemorrhagic fever infection in human body},
	volume = {1589},
	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4868769},
	doi = {10.1063/1.4868769},
	abstract = {The treatment model of DHF presented in this paper involves the dynamic of five time-dependent compartments, i.e. susceptible, infected, free virus particle, immune cell, and haematocrit level. The treatment model is investigated based on normalization of haematocrit level, which is expressed as intravenous fluid infusion control. We analyze the stability of the disease free equilibrium and the endemic equilibrium. The numerical simulations will explain the dynamic of each compartment in human body. These results show particularly that infected compartment and free virus particle compartment are tend to be vanished in two weeks after the onset of dengue virus. However, these simulation results also show that without the treatment, the haematocrit level will decrease even though not up to the normal level. Therefore the effective haematocrit normalization should be done with the treatment control.},
