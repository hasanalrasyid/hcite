}

@article{data_numerical_2014,
	title = {Numerical study of anomalous {TE}-polarized light scattering by metallic nanowires using realistic data},
	volume = {23},
	issn = {0218-8635},
	url = {http://www.worldscientific.com/doi/abs/10.1142/S0218863514500052},
	doi = {10.1142/S0218863514500052},
	abstract = {We report the result of a numerical study of surface plasmon induced anomalous behaviors in TE light scattering by single silver and gold nanowires of radius a. Going beyond the restricted case of nondissipative and nondispersive scatterers often reported previously, the current numerical calculation is performed directly on the basis of Mie's general formula, adopting the refractive index data of Johnson and Christy. Our result does not show the appearance of well resolved and multipole resonances in Qsca plotted against q(= 2πa/λ), for certain wire radii. It does show however, the growing contributions of the higher order modes as a increases. A series of closely-placed but well separated resonance curves nevertheless show up for varying wire radii within the range of small q, exhibiting systematic changes indicative of the size effects on the scattered waves. The further deduced Qsca(λ) spectra display the distinct resonance curves for different wire radii showing peculiar mix of monotonous and nonmonotonous variations of the resonance peak and spectral width with increasing a, as a result of complicated competitions among the growing contributions of the higher order modes. Finally, while the silver and gold scatterers appear to exhibit qualitatively similar behaviors, they differ largely in details due to the significantly different indices of refraction and dispersive properties.},
	number = {01},
	urldate = {2017-10-10},
	journal = {J. Nonlinear Optic. Phys. Mat.},
	author = {Data, G. Wesley Putra and Iskandar, Alexander A. and Tjia, May-On},
	month = mar,
	year = {2014},
	pages = {1450005},
}

