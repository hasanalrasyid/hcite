	journal = {AIP Conference Proceedings},
	author = {{Masturi} and Swardhani, Anggi Puspita and Sustini, Euis and Bukit, Minsyahril and {Mora} and {Khairurrijal} and Abdullah, Mikrajuddin},
	month = oct,
	year = {2010},
	pages = {59--63},
}

