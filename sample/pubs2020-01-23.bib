@article{yoshida_lightcurves_2016,
	title = {Lightcurves of the {Karin} family asteroids},
	volume = {269},
	issn = {0019-1035},
	url = {http://www.sciencedirect.com/science/article/pii/S0019103516000087},
	doi = {10.1016/j.icarus.2016.01.004},
	abstract = {The Karin family is a young asteroid family formed by an asteroid breakup 5.8 Myr ago. Since the members of this family probably have not experienced significant orbital or collisional evolution yet, it is possible that they still preserve properties of the original family-forming event in terms of their spin state. We carried out a series of photometric observations of the Karin family asteroids, and here we report on the analysis of the lightcurves including the rotation period of eleven members. The mean rotation rate of the Karin family members turned out to be much lower than those of near-Earth asteroids or small main belt asteroids (diameter D {\textless} 12 km), and even lower than that of large main belt asteroids (D {\textgreater} 130 km). We investigated a correlation between the peak-to-trough variation and the rotation period of the eleven Karin family asteroids, and found a possible trend that elongated members have lower spin rates, and less elongated members have higher spin rates. However, this trend has to be confirmed by another series of future observations.},
	number = {Supplement C},
