	month = nov,
	year = {2012},
	keywords = {Thorium fuel, Time-independent burnup, BWR, Void fraction, SRAC 2002},
	pages = {11--16},
}

@article{irwanto_burnup_2012,
	title = {Burnup characteristics and fuel cycle economics of mixed uranium–thorium fuel in a simplified small pebble bed reactor},
	volume = {49},
	issn = {0022-3131},
	url = {http://dx.doi.org/10.1080/00223131.2011.649080},
	doi = {10.1080/00223131.2011.649080},
	abstract = {The limitation of natural uranium resources and the improvement of economic values of nuclear reactors are important issues to be solved in the future development of these reactors. In our previous study, we presented an innovative design for simplifying a pebble bed reactor, and the optimization of this design showed that burnup values could be increased and natural uranium uses could be reduced. The purposes of the current study were to design a simplified pebble bed reactor by removing the unloading device from the reactor system and to further optimize the burnup characteristics of this reactor with a peu à peu fuel-loading scheme by introducing thorium in the fuel configuration as a fertile material. Another goal was to optimize the fuel composition so that the system could achieve even better burnup characteristics and use scarce uranium resources more efficiently. Using a specially developed computer code, we analyzed and optimized the performance of a 110-MWt simplified pebble bed reactor using a peu à peu fuel-loading scheme. An optimized design using 30\% of fertile thorium mixed with uranium fuel with 15\% 235U enrichment and a 7\% packing fraction calculated to achieve a high burnup of 140 GWD/T for more than 21 years' operation time that could save 13 to 33\% of natural uranium use compared with the savings noted in our previous study. Neutronic, burnup and fuel economic analysis for this optimized design are discussed in this study.},
	number = {2},
	urldate = {2017-09-28},
	journal = {Journal of Nuclear Science and Technology},
	author = {Irwanto, Dwi and Obara, Toru},
	month = feb,
	year = {2012},
	keywords = {Thorium fuel, peu à peu fuel-loading scheme, pebble bed reactor, neutronic, burnup},
	pages = {222--229},
}

