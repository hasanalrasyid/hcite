@article{arianto_parameterized_2010,
	title = {Parameterized {Post}‐{Newtonian} {Expansion} of {Scalar}‐{Vector}‐{Tensor} {Theory} of           {Gravity}},
	volume = {1244},
	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4757171},
	doi = {10.1063/1.4757171},
	abstract = {We investigate the weak‐field, post‐Newtonian expansion to the solution of the field equations in scalar‐vector‐tensor theory of gravity. In the calculation we restrict ourselves to the first post Newtonian. The parameterized post Newtonian (PPN) parameters are determined by expanding the modified field equations in the metric perturbation. Then, we compare the solution to the PPN formalism in first PN approximation proposed by Will and Nordtvedt and read of the coefficients (the PPN parameters) of post Newtonian potentials of the theory. We find that the values of γPPN and βPPN are the same as in General Relativity but the coupling functions β1, β2, and β3 are the effect of the preferred frame.},
	number = {1},
	urldate = {2017-09-27},
	journal = {AIP Conference Proceedings},
	author = {{Arianto} and Zen, Freddy P. and Gunara, Bobby E. and Hartanto, Andreas},
	month = jun,
	year = {2010},
