	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4724129},
	doi = {10.1063/1.4724129},
	abstract = {For a graph G(V,E), an edge-covering of G is a family of different subgraphs H1,…Hk such that any edge of E belongs to at least one of the subgraphs Hi, 1 ≤ i ≤ k. If every Hi is isomorphic to a given graph H, then G admits an H-covering. Graph G is said to be H-magic if G has an H-covering and there is a total labeling f:V∪E→\{1,2,…{\textbar}V{\textbar}+{\textbar}E{\textbar}\vphantom{\{}\} such that for each subgraph H′ = (V′,E′) of G isomorphic to H,ΣνϵV'f(ν)+ΣeϵE'f(e) is fixed constant. Furthermore, if f(V) = \}1,2,…{\textbar}V{\textbar}\{\vphantom{\}} then G is called H-supermagic. The sum of all vertex and edge labels on H, under a labeling f, is denoted by Σf(H). In this paper we study H-supermagic labeling for some classes of trees such as a double star, a caterpillar, a firecracker and a banana tree.},
	number = {1},
	urldate = {2017-09-28},
	journal = {AIP Conference Proceedings},
	author = {Roswitha, Mania and Baskoro, Edy Tri},
	month = may,
	year = {2012},
	pages = {135--138},
}

