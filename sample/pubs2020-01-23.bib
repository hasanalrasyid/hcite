	author = {Muhida, Rifki and Rahman, Md. Mamudur and Chowdhury, Md. Sazzad Hossein and Setiyanto, Henry and Zainuddin, Hishamuddin and Zakaria, Azmi Bin and Kasai, Hideaki},
