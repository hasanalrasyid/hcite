	title = {Revisiting galactic black hole binary {GX} 339-4 by using 2007 – 2014 {Swift} {XRT} observations},
	volume = {1677},
	issn = {0094-243X},
	url = {http://aip.scitation.org/doi/10.1063/1.4930669},
