@article{happyana_monitoring_2016,
	title = {Monitoring {Metabolite} {Profiles} of {Cannabis} sativa {L}. {Trichomes} during {Flowering} {Period} {Using} {1H} {NMR}-{Based} {Metabolomics} and {Real}-{Time} {PCR}},
