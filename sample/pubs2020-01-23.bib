	author = {Viridi, S. and Latief, F. D. E. and Khotimah, S. N.},
	year = {2017},
	pages = {012044},
}

@article{namari_first_2017,
	title = {First {Principle} {Calculation} : {Investigation} on interaction of {Pt}/{Graphene} as {Catalyst}},
